generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserProfile {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  firstname   String
  lastname    String
  email       String   @unique
  phone       String?
  address     Json?
  social      Json?
  preferences Json?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
